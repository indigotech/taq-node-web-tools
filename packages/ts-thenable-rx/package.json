{
  "name": "@taqtile/ts-thenable-rx",
  "version": "0.5.0",
  "description": "TypeScript thenable rx extensions",
  "homepage": "https://github.com/indigotech/taq-node-web-tools",
  "bugs": "https://github.com/indigotech/taq-node-web-tools/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/indigotech/taq-node-web-tools.git"
  },
  "author": "Taqtile",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "tsc",
    "test": "jest --colors --verbose --coverage",
    "postbuild": "yarn run copy-files && clean-to-publish",
    "copy-files": "copyfiles './README.md' './LICENSE' 'dist/'",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0"
  },
  "peerDependencies": {
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@taqtile/publish-package-json": "^0.5.0",
    "@types/jest": "24.0.12",
    "copyfiles": "2.1.0",
    "jest": "24.8.0",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "!src/**/index.ts",
      "src/**/*.ts"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist/*"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
