{
  "name": "@taqtile/ts-rx-extensions",
  "version": "0.1.0",
  "description": "TypeScript rx extensions",
  "repository": "https://github.com/indigotech/template-react.git",
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "Taqtile",
  "license": "MIT",
  "scripts": {
    "preinstall": "yarn cache clean @taqtile/publish-package-json",
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "tsc",
    "test": "jest --colors --verbose --coverage",
    "postbuild": "yarn run copy-files && clean-to-publish",
    "copy-files": "copyfiles './README.md' './LICENSE' 'dist/'"
  },
  "dependencies": {
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0"
  },
  "peerDependencies": {
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@taqtile/publish-package-json": "file:../publish-package-json",
    "@types/jest": "22.2.3",
    "copyfiles": "2.0.0",
    "jest": "22.4.4",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.6",
    "typescript": "2.8.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "collectCoverageFrom": [
      "!src/**/index.ts",
      "src/**/*.ts"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist/*"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
