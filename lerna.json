{
  "lerna": "2.11.0",
  "command": {
    "init": {
      "exact": true
    },
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): publish",
      "ignoreChanges": [
        "*.md"
      ]
    }
  },
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "version": "independent"
}
