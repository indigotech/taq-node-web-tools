{
  "version": "0.2.3",
  "command": {
    "init": {
      "exact": true
    },
    "publish": {
      "allowBranch": ["master", "feature/*"],
      "message": "chore(release): publish",
      "ignoreChanges": [
        "*.md"
      ]
    }
  },
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn"
}
