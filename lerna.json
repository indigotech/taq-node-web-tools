{
  "version": "0.5.0",
  "command": {
    "init": {
      "exact": true
    },
    "publish": {
      "allowBranch": [
        "master",
        "feature/*"
      ],
      "message": "chore(release): publish",
      "ignoreChanges": [
        "*.md"
      ]
    }
  },
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn"
}
